#!/bin/bash

#Mark Kocherovsky

#SBATCH --time=01:00:00
#SBATCH --nodes=1
#SBATCH --ntasks=1

#SBATCH --job-name=kocherov_cgp_40_run
#SBATCH -o "logs\/%x-%j.txt"
#SBATCH --mem=2G

module purge
cd $HOME
module load Conda/3
conda init bash
conda activate cgp

t=$1
g=$2
n=$3
p=$4
c=$5
f=$6

cd /mnt/home/kocherov/Documents/cgp/src/
echo "trial $t"
python3 -u cgp_40.py $t $g $n $p $c $f 
conda deactivate

