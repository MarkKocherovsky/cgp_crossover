#!/bin/bash

#Mark Kocherovsky

#SBATCH --time=02:00:00
#SBATCH --nodes=1
#SBATCH --ntasks=1

#SBATCH --job-name=kocherov_cgp_1x_run
#SBATCH -o "logs\/%x-%j.txt"
#SBATCH --mem=8G

module purge
cd $HOME
module load Conda/3
conda init bash
conda activate cgp

t=$1
