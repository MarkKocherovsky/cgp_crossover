n=$3
p=$4
c=$5
f=$6
ft=$7
pm=$8
pc=$9
cd /mnt/home/kocherov/Documents/cgp/src/
echo "trial $t"
python3 -u cgp_1x.py $t $g $n $p $c $f $ft $pm $pc
conda deactivate

