#!/bin/bash

#Mark Kocherovsky

#SBATCH --time=00:15:00
#SBATCH --nodes=1
#SBATCH --ntasks=1

#SBATCH --job-name=kocherov_lgp_run
#SBATCH -o "logs\/%x-%j.txt"
#SBATCH --mem=2G

module purge
cd $HOME
module load Conda/3
conda init bash
conda activate cgp

t=$1
g=$2
r=$3
d=$4
p=$5
c=$6
f=$7

cd /mnt/home/kocherov/Documents/cgp/launchers/
python3 -u ../src/lgp.py $t $g $r $d $p $c $f
conda deactivate

